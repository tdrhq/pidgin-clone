msgid "ping:  Send PING to the connected server"
msgstr "ping:  接続しているサーバへ PING を送信します"

#: src/protocols/silc/silc.c:1423
msgid "users &lt;channel&gt;:  List users in channel"
msgstr "users &lt;チャンネル&gt;:  チャンネル内のユーザを一覧表示します"

